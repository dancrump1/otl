{
  "require": {
    "born05/craft-assetusage": "4.1.3",
    "craftcms/ckeditor": "4.11.0",
    "craftcms/cms": "5.8.21",
    "craftcms/element-api": "4.2.0",
    "craftcms/security-patches": "dev-main",
    "doublesecretagency/craft-cpcss": "3.0.0",
    "ether/seo": "5.0.0",
    "revalidate/revalidate": "*",
    "roberskine/craft-user-manual": "5.0.4",
    "rynpsc/craft-phone-number": "3.0.0",
    "spicyweb/craft-embedded-assets": "5.4.4",
    "verbb/expanded-singles": "3.0.3",
    "verbb/field-manager": "4.0.3",
    "verbb/hyper": "2.2.6",
    "verbb/navigation": "3.0.15",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.10",
    "sserbin/twig-linter": "@dev",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "revalidate\\": "plugins/revalidate/src/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "composer/package-versions-deprecated": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "pre-craft-update": [],
    "post-craft-update": [
      "@php craft install/check && php craft up --interactive=0 || exit 0",
      "@php craft install/check && php craft clear-caches/all --interactive=0 || exit 0",
      "@php craft install/check && php craft invalidate-tags/all --interactive=0 || return 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    },
    {
      "type": "path",
      "url": "./plugins/revalidate",
      "options": {
        "symlink": true
      }
    }
  ]
}